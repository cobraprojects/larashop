{
    "name": "cobraprojects/larashop",
    "description": "Simple online store for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Melouk",
            "email": "m.melouk@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "cobraprojects/laravel-multiauth": "^2.0",
        "spatie/laravel-medialibrary": "^10.1.3",
        "cobraprojects/shoppingcart": "^4.1",
        "spatie/laravel-searchable": "1.11.0"
    },
    "autoload": {
        "psr-4": {
            "CobraProjects\\LaraShop\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^v7.1.0",
        "mockery/mockery": "^1.5.0"
    },
    "extra": {
        "laravel": {
