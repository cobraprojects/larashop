{
    "name": "cobraprojects/larashop",
    "description": "Simple online store for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Melouk",
            "email": "m.melouk@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "cobraprojects/laravel-multiauth": "2.*",
        "spatie/laravel-medialibrary": "^8.0.0",
        "bumbummen99/shoppingcart": "dev-master",
        "spatie/laravel-searchable": "dev-master",
        "intervention/image": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "CobraProjects\\LaraShop\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "mockery/mockery": "^1.4.3",
        "illuminate/support": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CobraProjects\\LaraShop\\LaraShopServiceProvider"
            ],
            "aliases": {
                "LaraShop": "CobraProjects\\LaraShop\\Facades\\laraShop"
            }
        }
    }
}
